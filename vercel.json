{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/proxy.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": ":path*",
      "destination": "https://live.devnimble.com:path*"
    }
  ]
}
